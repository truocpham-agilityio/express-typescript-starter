{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "server": "nodemon --delay 1 dist/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "nodemon --watch src --ext ts --exec 'tsc && node .'"
  },
  "keywords": [],
  "author": "Truoc Pham",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.8.0",
    "sequelize-typescript": "^2.1.1",
    "sqlite3": "^4.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/validator": "^13.6.5",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
